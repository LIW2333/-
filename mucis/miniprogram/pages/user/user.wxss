/* pages/user/user.wxss */
.userinfo {
  display: flex;
  align-items: center;
  padding-left: 280rpx;
  padding-top: 60rpx;
    flex-direction: column;

}

.userinfo-avatar {
  display: flex;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;

}

.userinfo-nickname {
  color: #aaa;
  font-size: 24px;
}
.container {
  background-color: #f7e5e7;
  height: 340rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: baseline;
  justify-content: space-between;
  padding: 0rpx 0;
  box-sizing: border-box;
} 
.page{
  background-color: rgb(199, 163, 177);

}